{
  "name": "swr-openapi",
  "version": "4.1.2",
  "description": "Generate SWR hooks from OpenAPI schemas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htunnicliff/swr-openapi.git"
  },
  "funding": "https://github.com/sponsors/htunnicliff",
  "author": {
    "name": "Hunter Tunnicliff"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "swr",
    "openapi",
    "rest",
    "generator",
    "client",
    "fetch"
  ],
  "scripts": {
    "prepublish": "npm run build && npm run types:check && npm run format:check",
    "build": "tsup",
    "test": "./setup-test-data.sh && tsc --project test/tsconfig.json",
    "types:check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "openapi-typescript-helpers": "0.0.13",
    "type-fest": "4.26.1"
  },
  "devDependencies": {
    "@types/react": "18.3.5",
    "expect-type": "0.20.0",
    "openapi-fetch": "0.12.0",
    "openapi-typescript": "7.4.0",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "swr": "2.2.5",
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "openapi-fetch": ">=0.12",
    "openapi-typescript": "7",
    "react": "18 || 19",
    "swr": "2"
  },
  "peerDependenciesMeta": {
    "openapi-typescript": {
      "optional": true
    }
  }
}
