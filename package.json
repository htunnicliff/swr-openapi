{
  "name": "swr-openapi",
  "type": "module",
  "version": "4.1.2",
  "description": "Generate SWR hooks from OpenAPI schemas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htunnicliff/swr-openapi.git"
  },
  "funding": "https://github.com/sponsors/htunnicliff",
  "author": {
    "name": "Hunter Tunnicliff"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": "./dist/query.js",
    "./immutable": "./dist/immutable.js",
    "./infinite": "./dist/infinite.js",
    "./mutate": "./dist/mutate.js",
    "./query": "./dist/query.js"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "swr",
    "openapi",
    "rest",
    "generator",
    "client",
    "fetch"
  ],
  "scripts": {
    "prepublish": "npm run build && npm run types:check && npm run format:check",
    "build": "tsc --project tsconfig.build.json",
    "test": "vitest",
    "types:check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "exports:check": "attw --pack . --ignore-rules=cjs-resolves-to-esm"
  },
  "dependencies": {
    "openapi-typescript-helpers": "0.0.13",
    "type-fest": "4.26.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.16.2",
    "@types/react": "18.3.5",
    "openapi-fetch": "0.12.0",
    "openapi-typescript": "7.4.0",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "swr": "2.2.5",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  },
  "peerDependencies": {
    "openapi-fetch": "0.12",
    "openapi-typescript": "7",
    "react": "18 || 19",
    "swr": "2"
  },
  "peerDependenciesMeta": {
    "openapi-typescript": {
      "optional": true
    }
  }
}
